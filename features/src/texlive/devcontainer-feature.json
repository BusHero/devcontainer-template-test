{
	"$schema": "https://raw.githubusercontent.com/devcontainers/spec/main/schemas/devContainerFeature.schema.json",
	"id": "texlive",
	"version": "2.0.0",
	"name": "texlive",
	"customizations": {
		"vscode": {
			"extensions": [
				"james-yu.latex-workshop"
			]
		}
	},
	"containerEnv": {
		"texlive_bin": "/opt/texlive/texdir/bin",
		"PATH": "${texlive_bin}/default:${PATH}"
	}
}
